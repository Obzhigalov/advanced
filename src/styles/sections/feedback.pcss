.feedback {
  z-index: 5;
  position: relative;
  padding-top: 130px;
  width: 70%;
  display: flex;
  flex-wrap: wrap;
  color: $white;
  min-height: 600px;
    @include tablet {
      width: 90%;
      margin-left: auto;
      margin-right: auto;
    }
}
.feedback__row {
  width: 100%;
  display: flex;
  align-items: center;
}
