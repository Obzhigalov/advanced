.portfolio {
  min-height: 1088px;
  overflow: hidden;
}
.portfolio .container {
  padding: 150px 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
    @include phone {
      width: 100%;
    }
}
.portfolio__title {
  width: 100%;
  color: $dark-blue;
  font-size: 36px;
  font-weight: 700;
    @include phone {
      text-align: center;
    }
}
